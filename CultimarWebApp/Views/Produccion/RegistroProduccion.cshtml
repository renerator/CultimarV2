
@{
    ViewBag.Title = "RegistroProduccion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 

<script src="~/WebJs/ProduccionJS/RegistroProduccion.js"></script>
 
    <div class="right_col" role="main">
        <h3>@ViewBag.Message</h3>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Listado de Registro de Producción</h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li>
                            <li>
                                <a class="close-link"><i class="fa fa-close"></i></a>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <p class="text-muted font-13 m-b-30">
                            Este listado muestra los Registro de produccion
                        </p>
                        <div class="table-responsive">
                            @{
        var grid = new WebGrid(Model);
                            }
                            @grid.GetHtml(
htmlAttributes: new { id = "tblRegistroProduccion" },
tableStyle: "table table-bordered",
columns:
grid.Columns(
grid.Column(columnName: "Id", header: "Id", format: @<text>@item.IdRegistroProduccion</text>),
grid.Column(columnName: "CantidadProductoresMachos", header: "Machos", format: @<text>@item.CantidadProductoresMachos</text>),
grid.Column(columnName: "CantidadProductoresHembras", header: "Hembras", format: @<text>@item.CantidadProductoresHembras</text>),
grid.Column(columnName: "FechaInicioCultivo", header: "F.Cultivo", format: @<text>@item.FechaInicioCultivo.ToString("MM/dd/yyyy")</text>),
grid.Column(columnName: "CantidadFecundada", header: "C.Fecundada", format: @<text>@item.CantidadFecundada</text>),
grid.Column(columnName: "NumeroDesoveTemporada", header: "N°Desove", format: @<text>@item.NumeroDesoveTemporada</text>),
grid.Column(columnName: "CantidadSembrada", header: "Sembrada", format: @<text>@item.CantidadSembrada</text>),
grid.Column(columnName: "FactoresMedicion", header: "F.Medición", format: @<text>@item.FactoresMedicion</text>),
grid.Column(columnName: "NumeroEstanquesUtilizado", header: "Estanques", format: @<text>@item.NumeroEstanquesUtilizado</text>),
grid.Column(columnName: "DensidadSiembra", header: "Siembra", format: @<text>@item.DensidadSiembra</text>), 
grid.Column(header: "Editar", format: @<button class="btn btn-success" type="button" data-toggle="modal" tooltip="Editar Parametro" data-target="#modalPopUpIngresoUsuario" 
                                               onclick="EditaProduccion(@item.IdRegistroProduccion,
                                               @item.CantidadProductoresMachos,
                                               @item.CantidadProductoresHembras, 
                                               '@item.FechaInicioCultivo',
                                               @item.CantidadFecundada,
                                               @item.NumeroDesoveTemporada,
                                               @item.CantidadSembrada,
                                               @item.FactoresMedicion,
                                               @item.NumeroEstanquesUtilizado,
                                               @item.DensidadSiembra)">Editar</button>))


)
                        </div>


                        <div id="divAlert"></div>
                        <div class="form-group">
                            <div>
                                <button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">Registro Pruducción</button>
                            </div>
                        </div>

                        <div id="modalPopUpIngresoUsuario" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
                            <div class="modal-dialog modal-lg">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal">
                                            <span aria-hidden="true">X</span>
                                        </button>
                                        <h4 class="modal-title" id="myModalLabel">Registo Inicial de Producción</h4>
                                    </div>
                                    <div class="modal-body">
                                        <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left">
                                           

                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                   Id Produción <span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="IdRegistro" value="-1" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Cant. Productores Machos <span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="CantidadPMachos" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Cant. Productores Hembras <span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="CantidadPHembras" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                                                    Fecha Cultivos <span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12 xdisplay_inputx form-group has-feedback">
                                                    <input id="single_cal1" name="FechaCultivos" type="text" class="form-control has-feedback-right" placeholder="Fecha" aria-describedby="inputSuccess2Status">
                                                    <span class="fa fa-calendar-o form-control-feedback right" aria-hidden="true"></span>
                                                    <span id="inputSuccess2Status" class="sr-only">(success)</span>

                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Cantidad Fecundada <span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="CantidadFecundada" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    N° Desove Temporada<span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="NroDesoveTemporada" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Cantidad Sembrada<span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="CantidadSembrada" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Factores de medición<span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="FactoresMedicion" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Numeros de Estanque<span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="NroEstanque" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="last-name">
                                                    Densidad de Siembra<span class="required">*</span>
                                                </label>
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <input type="text" id="DensidadSiembra" required="required" class="form-control col-md-7 col-xs-12">
                                                </div>
                                            </div>




                                            <div class="ln_solid"></div>
                                            <div class="form-group">
                                                <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
                                                    <button id="btnCerrarModal" data-dismiss="modal" class="btn btn-primary" type="button">Cancel</button>
                                                    <button type="submit" class="btn btn-success" id="btnGrabarDatos">Grabar</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" id="btnCerrarModal" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                                    </div>
                                </div>
                            </div>
                        </div>



                    </div>
                </div>
            </div>
        </div>
    </div>
