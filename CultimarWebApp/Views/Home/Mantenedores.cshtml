                                                                <button type="button" id="btnCerrarModalTipoAlimento" class="btn btn-default" data-dismiss="modal">Cerrar</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Fin Parametros  Ubicacion Oceanica-->
