
@{
    ViewBag.Title = "Mantenedores";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/WebJs/HomeJs/Mantenedores.js"></script>
<div class="right_col" role="main">
    <h3>@ViewBag.Message</h3>
