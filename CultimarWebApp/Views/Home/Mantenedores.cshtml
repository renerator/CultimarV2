                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <button type="button" class="close" data-dismiss="modal">
                                                                <span aria-hidden="true">×</span>
                                                            </button>
                                                            <h4 class="modal-title" id="myModalLabel">Agregar Origen</h4>
                                                        </div>
                                                        <div class="modal-body">
                                                            <h4>Ingrese los datos solicitados</h4>
                                                            <div class="x_content">
                                                                <br />
                                                                <input type="text" id="Idorigen" style="display: none;">
                                                                <form class="form-horizontal form-label-left input_mask">
                                                                    <div class="form-group">
                                                                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Nombre Parametro Origen</label>
                                                                        <div class="col-md-9 col-sm-9 col-xs-12">
                                                                            <input type="text" required="required" class="form-control" id="nombreOrigen">
                                                                        </div>
                                                                    </div>
                                                                    <div class="ln_solid"></div>
                                                                    <div class="form-group">
                                                                        <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
                                                                            <button class="btn btn-primary" type="reset">Resetear</button>
                                                                            <button type="submit" class="btn btn-success" id="btnGrabarParametrosOrigen">Grabar</button>
                                                                        </div>
                                                                    </div>
                                                                </form>
                                                            </div>
                                                        </div>
