gridParametrosOrigen.Column(header: "Editar", format: @<button class="btn btn-success" type="button" data-toggle="modal" tooltip="Editar Parametro" data-target="#modalPopUpIngresoOrigen" onclick="EditaOrigen(@item.IdOrigen,'@item.NombreOrigen')">Editar</button>))

