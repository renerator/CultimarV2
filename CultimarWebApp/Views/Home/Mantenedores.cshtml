                                        <h4 class="panel-title">Mantenedor de Origen</h4>
                                    </a>
                                    <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                        <div class="panel-body">
                                            <p>
                                                <strong>En esta sección veras un listado con los parametros de Origen, podras agregar uno nuevo si así lo requieres</strong>
                                            </p>
                                            <div class="table-responsive">
                                                @{
                                                    var gridParametrosOrigen = new WebGrid(source: ViewBag.ParametrosOrigen, canPage: false);
                                                }
                                                @gridParametrosOrigen.GetHtml(
                                                                        htmlAttributes: new { id = "tblParametrosOrigen" },
                                                                        tableStyle: "table table-bordered",
                                                                        columns:
                                                                            gridParametrosOrigen.Columns(
                                                                                gridParametrosOrigen.Column(header: "Identificador", format: @<text>@item.IdOrigen</text>),
                                                                                gridParametrosOrigen.Column(header: "Nombre Origen", format: @<text>@item.NombreOrigen</text>),
                                                                                gridParametrosOrigen.Column(header: "Estado", format: item => item.Estado ?
                                                                                    Html.Raw("<i class='fa fa-thumbs-up' data-toggle='tooltip' data-placement='top' title='' data-original-title='Parametro Activo'></i>") : Html.Raw("<i class='fa fa-thumbs-down' data-toggle='tooltip' data-placement='top' title='' data-original-title='Parametro Inactivo'></i>")),
                                                                                gridParametrosOrigen.Column(header: "Editar", format: @<button class="btn btn-success" type="button" data-toggle="modal" tooltip="Editar Parametro" data-target="#modalPopUpIngresoOrigen" onclick="EditaOrigen(@item.IdOrigen,'@item.NombreOrigen')">Editar</button>))

                                                                            )
                                            </div>
                                            <div class="form-group">
                                                <div>
                                                    <button type="reset" class="btn btn-primary" data-toggle="modal" data-target="#modalPopUpIngresoOrigen">Agregar Nuevo Origen</button>
                                                </div>
                                            </div>
                                            <div id="modalPopUpIngresoOrigen" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
