gridTipoContenedor.Columns(
gridTipoContenedor.Column(header: "Identificador", format: @<text>@item.IdContenedor</text>),
gridTipoContenedor.Column(header: "Nombre Contenedor", format: @<text>@item.NombreContenedor</text>),
gridTipoContenedor.Column(header: "Tipo Contenedor", format: @<text>@item.TipoContenedor</text>),
gridTipoContenedor.Column(header: "Estado", format: item => item.Estado ?
